{
  "name": "app",
  "productName": "app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "bastian2001",
    "email": "bas.c.spr@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-snap",
          "config": {
            "features": {
              "audio": true,
              "mpris": "com.example.mpris",
              "webgl": true
            },
            "summary": "Pretty Awesome"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/app/index.html",
                  "js": "./src/app/index.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/app/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/bull": "^3.14.0",
    "@types/node": "^13.11.1",
    "@types/node-schedule": "^1.3.0",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "css-loader": "^3.0.0",
    "electron": "8.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "sass": "^1.26.3",
    "sass-loader": "^9.0.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.7",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@types/parse-duration": "^0.1.0",
    "bull": "^3.16.0",
    "data-urls": "^2.0.0",
    "dot-prop": "^5.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^6.0.0",
    "formidable": "^1.2.2",
    "fs-extra": "^9.0.0",
    "graphql-request": "^2.0.0",
    "node-wifi": "^2.0.13",
    "parse-duration": "^0.4.4",
    "superzone": "^1.0.0",
    "systeminformation": "^4.25.1",
    "typedi": "^0.8.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.4",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.1.6",
    "vue-router-prefetch": "^1.6.0",
    "vue-svg-inline-loader": "^1.5.0"
  }
}
